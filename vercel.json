{
    "version": 2,
    "builds": [
      {
        "src": "api/proxy-server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/proxy-server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/build/index.html"
      }
    ]
  }  